# Public Google OAuth Client ID (Used in frontend)
NEXT_PUBLIC_CLIENT_ID=your_google_client_id_here

# Convex Deployment URL (Publicly used in frontend)
NEXT_PUBLIC_CONVEX_URL=your_convex_deployment_url_here

# Convex Deployment ID (Used in backend)
CONVEX_DEPLOYMENT=your_convex_deployment_id_here

# Eden AI API Key (Keep this secret, used in backend)
EDEN_AI_API_KEY=your_eden_ai_api_key_here

# Razorpay Keys (Used for payment processing)
RAZORPAY_LIVE_KEY=your_razorpay_live_key_here
RAZORPAY_SECRET_KEY=your_razorpay_secret_key_here
NEXT_PUBLIC_RAZORPAY_LIVE_KEY=your_razorpay_live_key_here

# Razorpay Subscription Plan ID
RAZORPAY_PLAN_ID=your_razorpay_plan_id_here
